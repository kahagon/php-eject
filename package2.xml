  <description>
eject support for PHP
  </description>

  <lead>
    <name>Keng-ichi Ahagon</name>
    <user>oasynnoum</user>
    <email>k.ahagon@n-3.so</email>
    <active>yes</active>
  </lead>

  <date>2012-10-08</date>
  <version>
    <release>0.0.1</release>
    <api>0.0.1</api>
  </version>
  <stability>
    <release>alpha</release>
    <api>alpha</api>
  </stability>

  <license uri="http://php.net/license" filesource="LICENSE">PHP</license>

  <notes>
- Initial release.

  </notes>

  <contents>
    <dir name="/">
      <file role='doc' name='EXPERIMENTAL'/>
      <file role='doc' name='CREDITS'/>
      <file role='src' name='config.m4'/>
      <file role='src' name='eject.dsp'/>
      <file role='src' name='config.w32'/>
      <file role='src' name='eject.c'/>
